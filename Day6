#include <iostream>
#include <string>
#include <algorithm> // For reverse
using namespace std;

int main() {
    int T;
    cin >> T;

    while (T--) {
        int K;
        string S;

        cin >> K;
        cin >> S;

        reverse(S.begin(), S.begin() + K);

        cout << S << endl;
    }

    return 0;
}
